GROUP=`id -gn`
ROOM=`echo $HOSTNAME | sed s/-pc[0-6][0-9]//`
if test -z "$ROOM"
then
 ROOM=`echo $test | sed s/-tpc//`
fi

export GROUP
export ROOM

if [ -e /home/softwares/linux/profiles/default ]
then
 . /home/softwares/linux/profiles/default
fi
if [ -e /home/softwares/linux/profiles/$ROOM ]
then
 . /home/softwares/linux/profiles/$ROOM
fi
if [ -e /home/softwares/linux/profiles/$GROUP ]
then
 . /home/softwares/linux/profiles/$GROUP
fi
if [ -e /home/softwares/linux/profiles/$USER ]
then
 . /home/softwares/linux/profiles/$USER
fi

